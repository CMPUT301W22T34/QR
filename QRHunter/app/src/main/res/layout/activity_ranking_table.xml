<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".RankingTable">

    <Button
        android:id="@+id/rank_to_menu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="2dp"
        android:layout_marginEnd="323dp"
        android:layout_marginBottom="7dp"
        android:text="Back"
        app:layout_constraintBottom_toTopOf="@+id/textView12"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="146dp"
        android:layout_height="44dp"
        android:layout_marginStart="34dp"
        android:layout_marginTop="7dp"
        android:layout_marginEnd="231dp"
        android:layout_marginBottom="3dp"
        android:text="Ranking"
        android:textSize="25sp"
        app:layout_constraintBottom_toTopOf="@+id/unique"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rank_to_menu" />

    <Button
        android:id="@+id/unique"
        android:layout_width="115dp"
        android:layout_height="60dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:text="Unique QR Code"
        app:layout_constraintEnd_toStartOf="@+id/numberScanned"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView12" />

    <Button
        android:id="@+id/numberScanned"
        android:layout_width="122dp"
        android:layout_height="62dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="8dp"
        android:text="Number Scanned"
        app:layout_constraintStart_toEndOf="@+id/unique"
        app:layout_constraintTop_toBottomOf="@+id/textView12" />

    <Button
        android:id="@+id/Total"
        android:layout_width="150dp"
        android:layout_height="64dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="116dp"
        android:layout_marginEnd="5dp"
        android:text="Total Score"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/numberScanned"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="408dp"
        android:layout_height="554dp"
        android:layout_marginEnd="2dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Total"></LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>