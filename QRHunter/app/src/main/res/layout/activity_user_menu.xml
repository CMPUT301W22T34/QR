<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".UserMenu">

    <TextView
        android:id="@+id/textView9"
        android:layout_width="132dp"
        android:layout_height="51dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="80dp"
        android:layout_marginEnd="235dp"
        android:layout_marginBottom="22dp"
        android:text="Welcome"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/my_button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/my_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="157dp"
        android:layout_marginTop="22dp"
        android:layout_marginEnd="131dp"
        android:layout_marginBottom="22dp"
        android:text="My profile"
        app:layout_constraintBottom_toTopOf="@+id/play_button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView9" />

    <Button
        android:id="@+id/play_button"
        android:layout_width="123dp"
        android:layout_height="43dp"
        android:layout_marginStart="157dp"
        android:layout_marginTop="11dp"
        android:layout_marginEnd="131dp"
        android:layout_marginBottom="18dp"
        android:text="Play"
        app:layout_constraintBottom_toTopOf="@+id/map_button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/my_button" />

    <Button
        android:id="@+id/map_button"
        android:layout_width="120dp"
        android:layout_height="48dp"
        android:layout_marginStart="157dp"
        android:layout_marginTop="7dp"
        android:layout_marginEnd="134dp"
        android:layout_marginBottom="10dp"
        android:text="Map"
        app:layout_constraintBottom_toTopOf="@+id/rank_button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/play_button" />

    <Button
        android:id="@+id/rank_button"
        android:layout_width="120dp"
        android:layout_height="47dp"
        android:layout_marginStart="157dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="134dp"
        android:layout_marginBottom="88dp"
        android:text="Rank"
        app:layout_constraintBottom_toTopOf="@+id/code_button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/map_button" />

    <Button
        android:id="@+id/code_button"
        android:layout_width="183dp"
        android:layout_height="44dp"
        android:layout_marginStart="118dp"
        android:layout_marginTop="83dp"
        android:layout_marginEnd="110dp"
        android:layout_marginBottom="8dp"
        android:text="Account Code"
        app:layout_constraintBottom_toTopOf="@+id/out_button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rank_button" />

    <Button
        android:id="@+id/out_button"
        android:layout_width="183dp"
        android:layout_height="48dp"
        android:layout_marginStart="118dp"
        android:layout_marginEnd="110dp"
        android:layout_marginBottom="38dp"
        android:text="Sign Out"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/code_button" />

    <SearchView
        android:layout_width="221dp"
        android:layout_height="47dp"
        android:layout_marginStart="13dp"
        android:layout_marginTop="77dp"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="47dp"
        app:layout_constraintBottom_toTopOf="@+id/my_button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView9"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>